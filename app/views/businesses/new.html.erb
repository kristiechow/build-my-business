<%= form_for @business, html: { multipart: true } do |f| %>

  <ul>
    <li>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </li>
    <li>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </li>
    <li>
      <%= f.label :category %>
      <%= f.text_field :category %>
    </li>
    <li>
      <%= f.label :location %>
      <%= f.text_field :location %>
    </li>
    <li>
      <%= f.label :photos %>
      <%= file_field_tag "images[]", type: :file, multiple: true %>
    </li>

    <li>
      <%= f.submit %>
    </li>
  </ul>

<% end %>
